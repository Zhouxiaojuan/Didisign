<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.peanut.jinbiao">

    <application
        android:name=".test.Helper"
        android:allowBackup="true"
        android:icon="@mipmap/icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Activity.MainActivity"
            android:label="Point"
            android:theme="@style/ImagepageTheme" />
        <activity
            android:name=".Activity.ImageRoom"
            android:theme="@style/ImagepageTheme"
            android:windowSoftInputMode="stateVisible|adjustPan" />
        <activity
            android:name=".Activity.LoginPager"
            android:theme="@style/TransLucentTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Activity.SignupPager"
            android:theme="@style/TransLucentTheme" />
        <activity android:name=".Activity.UserCenter"></activity>
    </application>

</manifest>